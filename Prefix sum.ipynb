   "source": [
    "def prefix_sum(array, i):\n",
    "    output = [0]\n",
    "    for element in range(i):\n",
    "        output.append(output[-1] + array[element])\n",
    "    output.pop(0)\n",
    "    return output\n",
    "\n",
    "prefix = prefix_sum([3,5,2,10], 4)\n",
    "\n",
    "print(prefix)\n",
    "        "
   ]
  }
